{
  "name": "@digitalroute/bagger",
  "version": "0.0.1",
  "description": "",
  "main": "dist/bagger.umd.js",
  "module": "dist/bagger.es5.js",
  "typings": "dist/types/bagger.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=9.11.2"
  },
  "scripts": {
    "commit": "git-cz",
    "eslint": "eslint -c .eslintrc.json 'lib/**/*.ts' 'test/**/*.ts'",
    "test": "ava",
    "test:watch": "ava --watch",
    "semantic-release": "semantic-release",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "prettier": "prettier --write \"*.{js,json,md,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitalroute/bagger.git"
  },
  "keywords": [
    "swagger",
    "build",
    "joi",
    "schema",
    "generator",
    "json",
    "json schema",
    "builder"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/digitalroute/bagger/issues"
  },
  "homepage": "https://github.com/digitalroute/bagger#readme",
  "dependencies": {
    "@types/hapi__joi": "^15.0.3",
    "joi": "^14.3.1",
    "joi-to-swagger": "^3.2.0",
    "json-schema-to-openapi-schema": "^0.3.0",
    "openapi3-ts": "^1.3.0"
  },
  "devDependencies": {
    "@hapi/joi": "^15.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "ava": "^2.2.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.1.0",
    "eslint-config-nodejs": "^2.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "lodash.camelcase": "^4.3.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.22.1",
    "semantic-release": "^15.13.19",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
